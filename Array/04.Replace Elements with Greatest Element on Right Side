//Replace Elements with Greatest Element on Right Side
//Input: arr = [17,18,5,4,6,1]
//Output: [18,6,6,6,1,-1]
//2 solutions
 
 vector<int> replaceElements(vector<int>& arr) {
        int n = arr.size();
        if(n<2) {
            arr[0]= -1;
            return arr;
        }
        
        //O(n), going reverse and checking max
        int m = -1;
        int t = arr[n-1];
        //arr[n-2]=arr[n-1];
        for(int i=n-2; i>=0;i--){
            m = max(m, t);
            t = arr[i];
            arr[i]=m;
        }
        arr[n-1]= -1;
        return arr;
        
        //O(n^2)
        // for(int i = 0; i<n;i++){
        //     int m = arr[i+1];
        //     for(int j = i+1; j<n;j++)
        //         m = max(m,arr[j+1]);
        //     arr[i]=m;
        // }
        // arr[n-1]= -1;
        // return arr;
        
    }
