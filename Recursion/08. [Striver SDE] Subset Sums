Given a list arr of N integers, print sums of all subsets in it.

void f(int n, int sum, vector<int> &arr, vector<int> &sumArr){
    if(n<0){
        sumArr.push_back(sum);
        return;
    }

    f(n-1, sum+arr[n], arr, sumArr);
    f(n-1, sum, arr, sumArr);

}

vector<int> subsetSums(vector<int> arr, int N)
{
    vector<int> sumArr;
    f(N-1, 0, arr, sumArr);
    sort(sumArr.begin(), sumArr.end());
    return sumArr;
}

--------------------------------------------------------------------
JavaScript:
- Take, not-take concept

rec(sum, sumArr, i, arr) {
    if(i < 0) {
        sumArr.push(sum);
        return;
    }
    this.rec(sum + arr[i], sumArr, i-1, arr); //Take
    this.rec(sum, sumArr, i-1, arr);    //Not take
}

subsetSums(arr) {
    let sumArr = [];
    this.rec(0, sumArr, arr.length - 1, arr);
    return sumArr;
}
