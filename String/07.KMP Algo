//Used to find pattern
//Based on LPS

int main()
{

    string s = "ababcababaad";
    int n = s.length();
    string p = "ababa";
    int m = p.length();
    int lps[n];
    fillLPS(s, lps);    //replace this with lps code
    int i=0;
    int j=0;
    while(i<n){
        if(s[i]==p[j]){
            i++;
            j++;
        }
        if(j== m){
            cout<<i-j<<" ";
            j = lps[j-1];
        }
        else if(i<n && s[i]!=p[j]){
            if(j==0){
                i++;
            }
            else{
                j = lps[j-1];
            }
        }
    }

    return 0;

}
